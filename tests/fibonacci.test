let println = @ffi[println].

let fib = lambda (n) {
	let iter =
		lambda (a, b, n) if n == 0
				   	  	 then a
				 		 else iter(b, a + b, n - 1).
	iter(0, 1, n)
}.

println(fib(1)).
println(fib(2)).
println(fib(3)).
println(fib(4)).
println(fib(5)).
println(fib(6)).
println(fib(7)).
println(fib(8)).

