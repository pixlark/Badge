let fib = lambda (n) {
	let iter =
		lambda (a, b, n) if n == 0
				   	  	 then a
				 		 else iter(b, a + b, n - 1).
	iter(0, 1, n)
}.

print fib(1).
print fib(2).
print fib(3).
print fib(4).
print fib(5).
print fib(6).
print fib(7).
print fib(8).

