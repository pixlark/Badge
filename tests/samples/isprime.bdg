let print = @builtin[print].
let println = @builtin[println].
let mod = @builtin[mod].

let isprime = lambda (n) {
	let helper = lambda (n, i) if   i == n
			   	 			   then 1
							   elif mod(n, i) == 0
		   	 			   	   then nothing
						   	   else helper(n, i + 1).
	helper(n, 2)
}.

let loop = lambda (n, m) {
	if n >= 50
	then nothing
	else {
		print(n).
		print(" ").
		if isprime(n)
		then print("prime!")
		else print("not prime").
		println("").
		loop(n + 1, m).
	}.
}.

loop(2, 50).
